<body>

<!-- Main Container Starts -->
	<div id="main-container" class="container">
	

	<!-- Main Heading Starts -->
		<h2 class="main-heading text-center">
			Register <br />
			<span>Create New Account</span>
		</h2>
	<!-- Main Heading Ends -->
	<!-- Registration Section Starts -->
		<section class="registration-area">
			<div class="row">
				<div class="col-sm-6">
				<!-- Registration Block Starts -->
					<div class="panel panel-smart">
						<div class="panel-heading">
							<h3 class="panel-title">Personal Information</h3>
						</div>
						
						
						<div class="panel-body">
						<!-- Registration Form Starts -->
								
								<%= form_for @user do |f| %>
  <% if @user.errors.any? %>
    <div class="error_messages">
      <h2>Form is invalid</h2>
      <ul>
        <% for message in @user.errors.full_messages %>
          <li><%= message %></li>
        <% end %>
      </ul>
    </div>
  <% end %>
  
 <div class="form-group" style = "padding-bottom: 30px " >
    
    <label  class="col-sm-3 control-label">First Name :</label>
    <div class="col-sm-9">
    <%= f.text_field :first_name,:class=>"form-control", placeholder: "First Name" %>
    </div>
  </div>
  
   <div class="form-group " style = "padding-bottom: 30px " >
    
    <label class="col-sm-3 control-label">Last Name :</label>
    <div class="col-sm-9">
    <%= f.text_field :last_name,:class=>"form-control", placeholder: "Last Name" %>
    </div>
  </div>
  
  <div class="form-group " style = "padding-bottom: 30px " >
    
    <label class="col-sm-3 control-label"> Email :</label>
    <div class="col-sm-9">
    <%= f.text_field :email,:class=>"form-control", placeholder: "Email" %>
    </div>
  </div>
  
    <div class="form-group " style = "padding-bottom: 30px " >
    
    <label class="col-sm-3 control-label"> Phone :</label>
    <div class="col-sm-9">
    <%= f.text_field :phone,:class=>"form-control", placeholder: "Phone" %>
    </div>
  </div>
  <h3 class="panel-heading inner">
									Delivery Information
								</h3>
	 <div class="form-group " style = "padding-bottom: 30px " >							
   <label class="col-sm-3 control-label"> Address :</label>
    <div class="col-sm-9">
    <%= f.text_field :address,:class=>"form-control", placeholder: "Address" %>
    </div>
  </div>
   <div class="form-group " style = "padding-bottom: 30px " >
   <label class="col-sm-3 control-label"> City :</label>
    <div class="col-sm-9">
    <%= f.text_field :city,:class=>"form-control", placeholder: "City" %>
    </div>
  </div>
  
  <h3 class="panel-heading inner">
									Password
								</h3>
	<div class="form-group " style = "padding-bottom: 30px " >
									<label class="col-sm-3 control-label">Password :</label>
									<div class="col-sm-9">
										<%= f.password_field :password,:class=>"form-control", placeholder: "Password" %>
									</div>
								</div>
								<div class="form-group " style = "padding-bottom: 30px " >
									<label class="col-sm-3 control-label">Re-Password :</label>
									<div class="col-sm-9">
										<%= f.password_field :password_confirmation,:class=>"form-control", placeholder: "Re-Password" %>
									</div>
								</div>
									
									<div class="form-group">
									<div class="col-sm-offset-3 col-sm-9">
									  <%= f.submit 'Register', :class => 'btn btn-brown' %>
									</div>
								</div>
								
  
<% end %>
					
								
					

							<!-- Password Area Ends -->
						<!-- Registration Form Starts -->
						</div>							
					</div>
				<!-- Registration Block Ends -->
				</div>
				<div class="col-sm-6">
				<!-- Conditions Panel Starts -->
					<div class="panel panel-smart">
						<div class="panel-heading">
							<h3 class="panel-title">
								Conditions
